@model ProjectNotesViewModel;
@{
	ViewData["Title"] = "Project Notes";
}

<div class="govuk-grid-row">
	<div class="govuk-grid-column-full">
		<span class="govuk-caption-xl">@Model.Project.SchoolURN</span>
		<h1 class="govuk-heading-xl govuk-!-margin-bottom-4">
			@Model.Project.SchoolName
			<strong class="govuk-tag govuk-tag--grey">@Model.Project.Phase</strong>
		</h1>
		<div class="moj-primary-navigation">
			<div class="govuk-!-margin-bottom-7">
				<partial name="/Views/Shared/_SubMenu.cshtml" model="Model.SubMenu" />

			</div>
		</div>
	</div>
</div>

